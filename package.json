{
  "name": "ngx-history",
  "version": "2.0.0",
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve",
    "build": "npx ng build",
    "test": "npx ng test ngx-history",
    "lint": "echo 'Linting not configured. Run: ng add @angular-eslint/schematics'",
    "test-ci": "npx ng test ngx-history --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "build-ci": "npx ng build ngx-history --configuration production",
    "build:npm": "npx ng build ngx-history",
    "build:demo": "npx ng build ngx-history --configuration production && npx ng build ngx-history-demo --base-href /ngx-history/ --configuration production",
    "deploy:demo": "npm run build:demo && npx angular-cli-ghpages --dir=dist/ngx-history-demo",
    "predeploy": "npm run build:demo"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.4",
    "@angular/common": "^21.1.4",
    "@angular/compiler": "^21.1.4",
    "@angular/core": "^21.1.4",
    "@angular/forms": "^21.1.4",
    "@angular/platform-browser": "^21.1.4",
    "@angular/platform-browser-dynamic": "^21.1.4",
    "@angular/router": "^21.1.4",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.4",
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.4",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^22.10.2",
    "angular-cli-ghpages": "^3.0.2",
    "jasmine-core": "^5.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^21.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
