{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-history": {
      "projectType": "library",
      "root": "projects/ngx-history",
      "sourceRoot": "projects/ngx-history/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-history/tsconfig.lib.json",
            "project": "projects/ngx-history/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-history/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-history/test.ts",
            "tsConfig": "projects/ngx-history/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-history/karma.conf.js"
          }
        }
      }
    },
    "ngx-history-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/ngx-history-demo",
      "sourceRoot": "projects/ngx-history-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/ngx-history-demo/src/main.ts",
            "tsConfig": "projects/ngx-history-demo/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ngx-history-demo/public"
              }
            ],
            "styles": [
              "projects/ngx-history-demo/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngx-history-demo:build:production"
            },
            "development": {
              "buildTarget": "ngx-history-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
